<rdf:RDF xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:xsd="http://www.w3.org/2001/XMLSchema#" xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <owl:Ontology rdf:about="http://jazz.net/ns/dm/rsa/deployment/core#">
    <dcterms:dateCopyrighted rdf:datatype="&xsd;string">2012</dcterms:dateCopyrighted>
    <dcterms:description rdf:datatype="&xsd;string">A vocabulary that defines a core set of concepts to describe Enterprise Architectures from the Software Applications down to the hardware requirements.</dcterms:description>
    <dcterms:license rdf:resource="https://jazz.net/ns/license.html" />
    <dcterms:title rdf:datatype="&xsd;string">Deployment</dcterms:title>
    <rdfs:seeAlso rdf:resource="https://jazz.net/help-dev/dm/index.jsp" />
  </owl:Ontology>
  <rdfs:Class rdf:about="http://jazz.net/ns/dm/rsa/deployment/core#Topology">
    <rdfs:comment rdf:datatype="&xsd;string">A container of Unit instances expressing a deployment topology.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://jazz.net/ns/dm/rsa/deployment/core#" />
    <rdfs:label rdf:datatype="&xsd;string">Topology</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://jazz.net/ns/dm/rsa/deployment/core#Unit">
    <rdfs:comment rdf:datatype="&xsd;string">Represents an abstract unit of publishable deployment. A unit can contain provided capabilities and consumed requirements. It is contained in a Topology.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://jazz.net/ns/dm/rsa/deployment/core#" />
    <rdfs:label rdf:datatype="&xsd;string">Unit</rdfs:label>
  </rdfs:Class>
</rdf:RDF>