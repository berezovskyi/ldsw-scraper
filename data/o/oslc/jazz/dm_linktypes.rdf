<?xml version="1.0" encoding="utf-16"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY rdf 'http://www.w3.org/1999/02/22-rdf-syntax-ns#'>
	<!ENTITY rdfs 'http://www.w3.org/2000/01/rdf-schema#'>
	<!ENTITY xsd 'http://www.w3.org/2001/XMLSchema#'>
	<!ENTITY owl 'http://www.w3.org/2002/07/owl#'>
	<!ENTITY dcterms 'http://purl.org/dc/terms/'>
]>
<rdf:RDF xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:xsd="http://www.w3.org/2001/XMLSchema#" xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <owl:Ontology rdf:about="http://jazz.net/ns/dm/linktypes#">
    <dcterms:dateCopyrighted rdf:datatype="&xsd;string">2012</dcterms:dateCopyrighted>
    <dcterms:description rdf:datatype="&xsd;string">Defines the properties that can be used in DM for OSLC linking.</dcterms:description>
    <dcterms:license rdf:resource="https://jazz.net/ns/license.html" />
    <dcterms:title rdf:datatype="&xsd;string">The DM Link Type Vocabulary.</dcterms:title>
    <rdfs:label rdf:datatype="&xsd;string">The DM Link Type Vocabulary.</rdfs:label>
  </owl:Ontology>
  <rdf:Property rdf:about="http://jazz.net/ns/dm/linktypes#derives">
    <rdfs:comment rdf:datatype="&xsd;string">The resource that derives from another resource originated from or is significantly influenced by the referenced resource. For example a model element derives from a requirement.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://jazz.net/ns/dm/linktypes#" />
    <rdfs:label rdf:datatype="&xsd;string">Derives From</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:about="http://jazz.net/ns/dm/linktypes#elaborates">
    <rdfs:comment rdf:datatype="&xsd;string">This resource elaborates the referenced resource.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://jazz.net/ns/dm/linktypes#" />
    <rdfs:label rdf:datatype="&xsd;string">Elaborates</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:about="http://jazz.net/ns/dm/linktypes#external">
    <rdfs:comment rdf:datatype="&xsd;string">A generic link from a resource to an external web page.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://jazz.net/ns/dm/linktypes#" />
    <rdfs:label rdf:datatype="&xsd;string">External Web Page</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:about="http://jazz.net/ns/dm/linktypes#refine">
    <rdfs:comment rdf:datatype="&xsd;string">The target is a refinement of the source. (e.g. a use case scenario might be a refinement of a textual requirement that describes the interaction).</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://jazz.net/ns/dm/linktypes#" />
    <rdfs:label rdf:datatype="&xsd;string">Refines</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:about="http://jazz.net/ns/dm/linktypes#satisfy">
    <rdfs:comment rdf:datatype="&xsd;string">The model element satisfies the requirement (e.g. The use case satisfies a functional requirement).</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://jazz.net/ns/dm/linktypes#" />
    <rdfs:label rdf:datatype="&xsd;string">Satisfies</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:about="http://jazz.net/ns/dm/linktypes#trace">
    <rdfs:comment rdf:datatype="&xsd;string">The model element has a trace to the requirement (e.g. An attribute or its value are traced to a requirement).</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://jazz.net/ns/dm/linktypes#" />
    <rdfs:label rdf:datatype="&xsd;string">Trace</rdfs:label>
  </rdf:Property>
</rdf:RDF>