<rdf:RDF xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:xsd="http://www.w3.org/2001/XMLSchema#" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:rtcp="http://jazz.net/ns/qm/rtcp#" xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <owl:Ontology rdf:about="&rtcp;">
    <dcterms:dateCopyrighted rdf:datatype="&xsd;string">2013</dcterms:dateCopyrighted>
    <dcterms:description rdf:datatype="&xsd;string">The Rational Test Control Panel vocabulary defines concepts related to Service Virtualization. The URI for this vocabulary is also used to identify the Rational Test Control Panel product itself.</dcterms:description>
    <dcterms:license rdf:resource="https://jazz.net/ns/license.html" />
