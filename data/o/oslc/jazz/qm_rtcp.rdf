  </owl:Ontology>
  <rdf:Description rdf:about="&rtcp;ServiceVirtualization">
    <rdfs:comment rdf:datatype="&xsd;string">Service Virtualization. This term can be used as the oslc:usage value on an oslc:ServiceProvider or oslc:Service to indicate that the service(s) provide control or management of Virtual Services.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="&rtcp;" />
    <rdfs:label rdf:datatype="&xsd;string">ServiceVirtualization</rdfs:label>
    <rdfs:seeAlso rdf:resource="http://www.ibm.com/software/rational/servicevirtualization/" />
  </rdf:Description>
