      }
    ],
    "http://www.w3.org/2000/01/rdf-schema#label": [
      {
        "@value": "AdministrableResource"
      }
    ],
    "http://www.w3.org/2000/01/rdf-schema#comment": [
      {
        "@value": "It describes a resource that is administered.  The resource can be an instance of an application, service, database, middleware, operating system, etc.  It represents the target for the admin:AdministrativeJob."
      }
    ]
  },
