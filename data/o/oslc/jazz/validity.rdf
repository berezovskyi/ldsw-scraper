<?xml version="1.0" encoding="utf-16"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY rdf 'http://www.w3.org/1999/02/22-rdf-syntax-ns#'>
	<!ENTITY rdfs 'http://www.w3.org/2000/01/rdf-schema#'>
	<!ENTITY xsd 'http://www.w3.org/2001/XMLSchema#'>
	<!ENTITY owl 'http://www.w3.org/2002/07/owl#'>
	<!ENTITY oslc 'http://open-services.net/ns/core#'>
	<!ENTITY dcterms 'http://purl.org/dc/terms/'>
	<!ENTITY validity 'http://jazz.net/ns/validity#'>
]>
<rdf:RDF xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:xsd="http://www.w3.org/2001/XMLSchema#" xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:oslc="http://open-services.net/ns/core#" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:validity="http://jazz.net/ns/validity#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <owl:Ontology rdf:about="&validity;">
    <dcterms:dateCopyrighted rdf:datatype="&xsd;string">2015</dcterms:dateCopyrighted>
    <dcterms:description rdf:parseType="Literal">The vocabulary terms defined in the validity namespace.</dcterms:description>
    <dcterms:license rdf:resource="https://jazz.net/ns/license.html" />
    <dcterms:source rdf:resource="https://jazz.net/wiki/pub/LinkedData/JazzLinkValidityVocabulary/LinkValidityVocabulary.ttl" />
    <dcterms:title rdf:datatype="&xsd;string">Defines the vocabulary for validity elements.</dcterms:title>
    <rdfs:label rdf:datatype="&xsd;string">validity</rdfs:label>
  </owl:Ontology>
  <validity:ValidityStatus rdf:about="&validity;Invalid">
    <rdfs:comment rdf:datatype="&xsd;string">Invalid status</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="&validity;" />
    <rdfs:label rdf:datatype="&xsd;string">Invalid</rdfs:label>
  </validity:ValidityStatus>
  <rdfs:Class rdf:about="&validity;LinkValidity">
    <rdfs:comment rdf:datatype="&xsd;string">Information about the validity of a link.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="&validity;" />
    <rdfs:label rdf:datatype="&xsd;string">Link Validity</rdfs:label>
  </rdfs:Class>
  <validity:ValidityStatus rdf:about="&validity;Valid">
    <rdfs:comment rdf:datatype="&xsd;string">Valid status</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="&validity;" />
    <rdfs:label rdf:datatype="&xsd;string">Valid</rdfs:label>
  </validity:ValidityStatus>
  <rdfs:Class rdf:about="&validity;ValidityProfile">
    <rdfs:comment rdf:datatype="&xsd;string">A profile under which link validity may be established.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="&validity;" />
    <rdfs:label rdf:datatype="&xsd;string">Validity Profile</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="&validity;ValidityStatus">
    <rdfs:comment rdf:datatype="&xsd;string">The validity status of a link (valid or invalid).</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="&validity;" />
    <rdfs:label rdf:datatype="&xsd;string">Validity Status</rdfs:label>
  </rdfs:Class>
  <rdf:Property rdf:about="&validity;contentHash">
    <rdfs:comment rdf:datatype="&xsd;string">The hash of the contents of a resource.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="&validity;" />
    <rdfs:label rdf:datatype="&xsd;string">Content hash value</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:about="&validity;profile">
    <rdfs:comment rdf:datatype="&xsd;string">The profile under which the link validity has been established.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="&validity;" />
    <rdfs:label rdf:datatype="&xsd;string">Profile</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:about="&validity;status">
    <rdfs:comment rdf:datatype="&xsd;string">The validity status of the link (valid or invalid).</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="&validity;" />
    <rdfs:label rdf:datatype="&xsd;string">Status</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:about="&validity;validateSourceHash">
    <rdfs:comment rdf:datatype="&xsd;string">The source hash of the link to be validated.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="&validity;" />
    <rdfs:label rdf:datatype="&xsd;string">validateSourceHash</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:about="&validity;validateTargetHash">
    <rdfs:comment rdf:datatype="&xsd;string">The target hash of the link to be validated.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="&validity;" />
    <rdfs:label rdf:datatype="&xsd;string">validateTargetHash</rdfs:label>
  </rdf:Property>
</rdf:RDF>