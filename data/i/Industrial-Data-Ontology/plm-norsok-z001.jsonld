    "broadMatch" : [ "rdl:PCA_100006712", "rdl:PCA_100006702" ],
    "broader" : "rdl:PCA_100006745"
  }, {
    "@id" : "rdl:PCA_100006177",
    "@type" : "owl:Class",
    "PCA_100006763" : "The diagram shall show the electrical connection between different electrical equipment and components within a package. The diagram shall also identify all connections to Company’s power supply and instrumented systems.",
    "PCA_100006764" : "Electrical connection diagram shall typical include the following:\n - Tag number of equipment to which the cable ends are terminated;\n - Signal tag number;\n - External cable tag number;\n - External cables with pair or wire number and core colour details;\n - Terminal block/socket number;\n - Earthing details;\n - Reference drawings.",
    "PCA_100006765" : [ "rrdl:CFIHOS-20000031", "rrdl:CFIHOS-20000030", "rrdl:CFIHOS-20000008" ],
    "PCA_100006766" : [ "rdl:PCA_100006637", "rdl:PCA_100006636", "rdl:PCA_100006639", "rdl:PCA_100006638", "rdl:PCA_100006642", "rdl:PCA_100006641", "rdl:PCA_100006635", "rdl:PCA_100006640", "rdl:PCA_100006634" ],
    "label" : "Electrical connection diagram",
    "subClassOf" : "rdl:PCA_100004908",
