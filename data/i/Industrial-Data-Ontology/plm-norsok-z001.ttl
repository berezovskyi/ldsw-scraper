###  http://rds.posccaesar.org/ontology/plm/rdl/PCA_100006087
rdl:PCA_100006087 rdf:type owl:Class ;
                  rdfs:subClassOf rdl:PCA_100004908 ;
                  rdl:PCA_100006763 "The ALARP register should be established to keep track of identification, evaluation and decision regarding risk reducing measures." ;
                  rdl:PCA_100006765 rrdl:CFIHOS-20000010 ,
                                    rrdl:CFIHOS-20000011 ,
                                    rrdl:CFIHOS-20000013 ;
                  rdl:PCA_100006766 rdl:PCA_100006626 ,
                                    rdl:PCA_100006627 ,
                                    rdl:PCA_100006628 ,
                                    rdl:PCA_100006629 ,
                                    rdl:PCA_100006630 ,
                                    rdl:PCA_100006631 ,
                                    rdl:PCA_100006632 ,
                                    rdl:PCA_100006633 ;
                  rdl:PCA_100006768 "An ALARP register could contain the following information fields:\n - Unique identification number;\n - Source of identification, e.g. Hazop;\n - Cross reference to other databases/registers, for instance the uncertainty management register;\n - Description of the hazard/risk to be prevented or mitigated;\n - Description of the risk reducing measure;\n - Relevant DSHA(s);\n - Relevant safety barrier(s);\n - Coarse risk evaluation;\n - Decision based on coarse risk evaluation;\n - Date and signature for coarse risk evaluation/decision;\n - Detailed risk evaluation, if applicable;\n - Decision based on detailed risk evaluation, if applicable;\n - Date and signature for detailed risk evaluation/decision, if applicable." ;
                  rdfs:comment "An ALARP register could contain the following information fields:\n - Unique identification number;\n - Source of identification, e.g. Hazop;\n - Cross reference to other databases/registers, for instance the uncertainty management register;\n - Description of the hazard/risk to be prevented or mitigated;\n - Description of the risk reducing measure;\n - Relevant DSHA(s);\n - Relevant safety barrier(s);\n - Coarse risk evaluation;\n - Decision based on coarse risk evaluation;\n - Date and signature for coarse risk evaluation/decision;\n - Detailed risk evaluation, if applicable;\n - Decision based on detailed risk evaluation, if applicable;\n - Date and signature for detailed risk evaluation/decision, if applicable." ;
                  rdfs:label "ALARP register" ;
                  skos:broadMatch rdl:PCA_100006726 ,
                                  rdl:PCA_100006727 ;
                  skos:broader rdl:PCA_100006748 .


###  http://rds.posccaesar.org/ontology/plm/rdl/PCA_100006088
rdl:PCA_100006088 rdf:type owl:Class ;
                  rdfs:subClassOf rdl:PCA_100004908 ;
