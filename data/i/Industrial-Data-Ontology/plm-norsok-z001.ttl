###  http://rds.posccaesar.org/ontology/plm/rdl/PCA_100006090
rdl:PCA_100006090 rdf:type owl:Class ;
                  rdfs:subClassOf rdl:PCA_100004908 ;
                  rdl:PCA_100006763 "An index describing the scope of Anodes for an installation, and reference related information." ;
                  rdl:PCA_100006765 rrdl:CFIHOS-20000031 ;
                  rdl:PCA_100006766 rdl:PCA_100006653 ;
                  rdl:PCA_100006768 "An anode/reference electrode index shall be provided containing the following information:\n - Anode/ref. electrode type (unique reference to applicable anode/ref. electrode datasheet);\n - Data sheet number (data sheet to include details of dimensions, weight, Ex. cert., potential (max/min), sacrificial material, steel insert quality;\n - PO number;\n - Manufacturer;\n - Manufacturer type;\n - Location;\n - Identification reference; cross reference to layout drawing for anode/ref. electrode;\n - Layout drawing number." ;
                  rdfs:comment "An anode/reference electrode index shall be provided containing the following information:\n - Anode/ref. electrode type (unique reference to applicable anode/ref. electrode datasheet);\n - Data sheet number (data sheet to include details of dimensions, weight, Ex. cert., potential (max/min), sacrificial material, steel insert quality;\n - PO number;\n - Manufacturer;\n - Manufacturer type;\n - Location;\n - Identification reference; cross reference to layout drawing for anode/ref. electrode;\n - Layout drawing number." ;
                  rdfs:label "Anode index" ;
                  skos:broadMatch rdl:PCA_100006716 ,
