rdl:PCA_100006287 rdf:type owl:Class ;
                  rdfs:subClassOf rdl:PCA_100004908 ;
                  rdl:PCA_100006763 "Design report covering insulation joint design and requirements. The Insulating joints design report shall reflect the requirements for design in ISO 15589-1." ;
