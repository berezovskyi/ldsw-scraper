    <owl:annotatedTarget rdf:resource="http://purl.obolibrary.org/obo/RO_0002536"/>
    <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#seeAlso"/>
    <owl:annotatedSource rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/datumUOM"/>
