  <owl:ObjectProperty rdf:about="http://rds.posccaesar.org/ontology/lis14/rdl/uomOfDatum">
    <iof-av:usageNote>For annotations, see the inverse relation 'datumUOM'.</iof-av:usageNote>
    <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >false</iof-av:isPrimitive>
    <rdfs:seeAlso>
      <owl:FunctionalProperty rdf:about="http://rds.posccaesar.org/ontology/lis14/rdl/datumUOM"/>
    </rdfs:seeAlso>
    <rdfs:label>uomOfDatum</rdfs:label>
    <rdfs:comment>Relation from 'unit of measure' to 'quantity datum'. The inverse relation of datumUOM.</rdfs:comment>
    <owl:inverseOf>
      <owl:FunctionalProperty rdf:about="http://rds.posccaesar.org/ontology/lis14/rdl/datumUOM"/>
    </owl:inverseOf>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://rds.posccaesar.org/ontology/lis14/rdl/contentPartOf"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://rds.posccaesar.org/ontology/lis14/rdl/hasPart">
    <iof-av:naturalLanguageDefinition>If x 'has part' y, then y is a part of x.</iof-av:naturalLanguageDefinition>
    <rdfs:seeAlso rdf:resource="http://purl.obolibrary.org/obo/BFO_0000178"/>
    <rdfs:label>hasPart</rdfs:label>
    <iof-av:usageNote>Where possible, the sub-properties should be used. This property can be used for arbitrary aggregates.</iof-av:usageNote>
    <iof-av:primitiveRationale>Parthood is a primitive for ontologies.</iof-av:primitiveRationale>
    <rdfs:seeAlso rdf:resource="http://purl.obolibrary.org/obo/BFO_0000117"/>
    <rdfs:seeAlso>ISO 15926-2:2003 'composition_of_individual'</rdfs:seeAlso>
    <iof-av:usageNote>The relationship x 'has part' y implies that y  is physically or conceptually associated with x, contributing to its  structure, functionality, or characteristics.</iof-av:usageNote>
    <rdfs:seeAlso rdf:resource="http://www.w3.org/ns/ssn/hasSubSystem"/>
    <rdfs:seeAlso rdf:resource="http://purl.obolibrary.org/obo/BFO_0000051"/>
    <skos:example>A system has functional parts, a process has stages, a plant is partitioned into areas.</skos:example>
    <rdfs:seeAlso rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#part"/>
    <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</iof-av:isPrimitive>
    <rdfs:seeAlso>
      <owl:ObjectProperty rdf:about="http://rds.posccaesar.org/ontology/lis14/rdl/partOf"/>
    </rdfs:seeAlso>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://rds.posccaesar.org/ontology/lis14/rdl/partOf">
    <iof-av:usageNote>For annotations, see the inverse relation 'has part'.</iof-av:usageNote>
    <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >false</iof-av:isPrimitive>
    <rdfs:seeAlso rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/hasPart"/>
    <rdfs:label>partOf</rdfs:label>
    <owl:inverseOf rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/hasPart"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://rds.posccaesar.org/ontology/lis14/rdl/realizes">
    <iof-av:usageNote>For annotations, see the inverse relation 'realized in'.</iof-av:usageNote>
    <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >false</iof-av:isPrimitive>
    <rdfs:seeAlso>
      <owl:ObjectProperty rdf:about="http://rds.posccaesar.org/ontology/lis14/rdl/realizedIn"/>
    </rdfs:seeAlso>
    <rdfs:seeAlso rdf:resource="http://purl.obolibrary.org/obo/BFO_0000055"/>
    <rdfs:label>realizes</rdfs:label>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://rds.posccaesar.org/ontology/lis14/rdl/realizedIn"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://rds.posccaesar.org/ontology/lis14/rdl/quantityDatumOfProfile">
    <iof-av:usageNote>For annotations, see the inverse relation 'profile quantity datum'.</iof-av:usageNote>
    <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >false</iof-av:isPrimitive>
    <rdfs:seeAlso>
      <owl:ObjectProperty rdf:about="http://rds.posccaesar.org/ontology/lis14/rdl/profileQuantityDatum"/>
    </rdfs:seeAlso>
    <rdfs:label>quantityDatumOfProfile</rdfs:label>
