    <rdfs:isDefinedBy>https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/</rdfs:isDefinedBy>
  </owl:AnnotationProperty>
  <owl:Axiom>
    <rdfs:comment>obo:BFO_0000148 is 'zero-dimensional temporal region'</rdfs:comment>
    <owl:annotatedTarget rdf:resource="http://purl.obolibrary.org/obo/BFO_0000148"/>
