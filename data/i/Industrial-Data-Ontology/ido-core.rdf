    <rdfs:isDefinedBy rdf:resource="http://purl.org/pav/"/>
  </owl:AnnotationProperty>
  <owl:Axiom>
    <rdfs:comment>BFO_0000171 is 'located in at some time' in BFO-ISO. This relation has domain and range restricted to 'independent continuant' and not 'spatial region', meaning it's a relation between entities that are material objects or determined by material objects, only.</rdfs:comment>
    <owl:annotatedTarget rdf:resource="http://purl.obolibrary.org/obo/BFO_0000171"/>
    <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#seeAlso"/>
